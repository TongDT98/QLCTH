        formData.append("fileName", $("#fileupload").val());
        formData.append("file", $("#fileupload")[0].files[0]);
        $.ajax({
            url: "@Url.Action("DoImportExcel")",
            data: formData,
            contentType: false,
            processData: false,
            type: "post",
            success: function (response) {
